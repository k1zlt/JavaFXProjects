<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>


<AnchorPane prefHeight="900.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uca.org.javafx.WebBrowser.Controller">
   <children>
      <WebView fx:id="webView" layoutY="24.0" prefHeight="879.0" prefWidth="1500.0" />
      <Button layoutX="1098.0" layoutY="2.0" mnemonicParsing="false" onAction="#loadPage" text="Search" />
      <TextField fx:id="textField" layoutX="3.0" layoutY="2.0" prefHeight="24.0" prefWidth="1095.0" />
      <Button fx:id="reloadBtn" layoutX="1161.0" layoutY="2.0" mnemonicParsing="false" onAction="#refreshPage" text="Reload" />
      <Button layoutX="1224.0" layoutY="2.0" mnemonicParsing="false" onAction="#zoomIn" text="ZoomIn" />
      <Button layoutX="1286.0" layoutY="2.0" mnemonicParsing="false" onAction="#zoomOut" text="ZoomOut" />
      <Button layoutX="1366.0" layoutY="2.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Button layoutX="1416.0" layoutY="2.0" mnemonicParsing="false" onAction="#forward" text="Forward" />
   </children>
</AnchorPane>
